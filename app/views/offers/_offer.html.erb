<div class="col-md-3">
        <%= link_to(offer) do %>
          <div class="thumbnail offer">
            <p><%= offer.description %></p>
            <%= image_tag offer.image, class: "img-responsive", style: "max-height:165px", alt: "" %>
            <p>
              Offered by <%= offer.user.first_name%>
            </p>
          </div>
        <% end %>
</div>
